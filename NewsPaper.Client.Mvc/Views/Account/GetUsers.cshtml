@using Calabonga.OperationResults
@using NewsPaper.Client.Mvc.ViewModels.User;
@model OperationResult<IEnumerable<UserViewModel>>

<partial name="_menuAccounts" />

<h1>GetUsers</h1>

<div class="row row-cols-1 row-cols-md-2">
    @foreach (var user in Model.Result)
    {
        <div class="col">
            <div class="card" style="width: 30rem; margin-bottom: 30px;">
                <div class="card-body">
                    <h5 class="card-title">@user.NikeName</h5>
                </div>
            </div>
        </div>
    }
</div>